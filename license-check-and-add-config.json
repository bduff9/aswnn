{
  "ignore": [
    "*.txt",
    ".env",
    ".env.example",
    ".eslintignore",
    ".gitignore",
    ".eslintrc.js",
    ".nvmrc",
    ".ts-prunerc",
    "LICENSE",
    "README.md",
    "*.json",
    "*.yml",
    ".gitignore",
    ".husky/**",
    ".github/**",
    "__sapper__/**",
    "node_modules/**",
    "s3/**",
    ".vscode/**",
    ".husky/**",
    ".github/**"
  ],
  "license": "LICENSE-HEAD",
  "licenseFormats": {
    "json|js|ts|scss|css": {
      "prepend": "/*******************************************************************************",
      "append": " */",
      "eachLine": {
        "prepend": " * "
      }
    },
    "dotfile|^Dockerfile": {
      "eachLine": {
        "prepend": "# "
      }
    },
    "html|svelte": {
      "append": "-->",
      "prepend": "<!--"
    },
    "default": {
      "append": "*/",
      "prepend": "/*"
    }
  },
  "trailingWhitespace": "TRIM"
}
